{
  "name": "project-template-react",
  "version": "0.2.0",
  "private": true,
  "dependencies": {
    "@appquality/appquality-design-system": "^1.0.49",
    "@appquality/craft-blocks": "^0.1.27",
    "@datadog/browser-logs": "^3.4.1",
    "@reduxjs/toolkit": "^1.8.2",
    "formik": "^2.2.6",
    "i18n-iso-countries": "^6.7.0",
    "i18next": "^20.2.2",
    "i18next-browser-languagedetector": "^6.1.0",
    "lint-staged": "^10.5.4",
    "lzutf8": "^0.6.0",
    "query-string": "^7.0.0",
    "react": "^17.0.2",
    "react-app-polyfill": "^2.0.0",
    "react-bootstrap-icons": "^1.4.0",
    "react-dom": "^17.0.2",
    "react-gtm-module": "^2.0.11",
    "react-helmet": "^6.1.0",
    "react-i18next": "^11.8.15",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-select": "^4.3.1",
    "redux": "^4.1.1",
    "redux-saga": "^1.1.3",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.3.0",
    "ts-node": "^10.8.1",
    "universal-cookie": "^4.0.4",
    "uuid": "^8.3.2",
    "web-vitals": "^1.0.1",
    "yup": "^0.32.9"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "prepare": "npx husky install",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "craco start",
    "build": "react-scripts build",
    "test": "test:e2e && test:unit",
    "test:e2e": "start-server-and-test start http://127.0.0.1:3000/ cy:run",
    "test:ci": "start-server-and-test start http://127.0.0.1:3000/ cy:run",
    "test:unit": "react-scripts test",
    "cy:run": "cypress run --e2e --browser chrome",
    "cy:open": "cypress open --e2e --browser chrome",
    "prettier": "npx prettier --write 'src/**/*.{js,jsx,ts,tsx,json,css,scss,md}'",
    "translate": "i18next-scanner",
    "generate-schema": ". ./.env.development; npx openapi-typescript $REACT_APP_API_URL/reference/ --output src/utils/schema.ts ",
    "generate-api": ". ./.env.development; API_URL=$REACT_APP_API_URL/reference/ npx @rtk-query/codegen-openapi src/services/tryberApi/config.ts",
    "generate-fixtures": ". ./.env.development; API_URL=$REACT_APP_API_URL/reference/; npx @appquality/fixture-generator -u $API_URL"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@appquality/fixture-generator": "^1.0.2",
    "@babel/core": "^7.13.0",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@craco/craco": "^6.4.2",
    "@redux-devtools/core": "^3.11.0",
    "@rtk-query/codegen-openapi": "^1.0.0-alpha.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.0.0",
    "@types/node": "^14.14.41",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-gtm-module": "^2.0.0",
    "@types/react-helmet": "^6.1.1",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-select": "^4.0.15",
    "@types/react-select-country-list": "^2.2.0",
    "@types/styled-components": "^5.1.9",
    "@types/uuid": "^8.3.1",
    "cypress": "^13.3.1",
    "file-loader": "^6.2.0",
    "husky": "^6.0.0",
    "i18next-scanner": "^3.1.0",
    "i18next-scanner-typescript": "^1.0.6",
    "jest-canvas-mock": "^2.4.0",
    "msw": "^0.47.4",
    "openapi-typescript": "^5.1.1",
    "prettier": "^2.3.1",
    "react-scripts": "4.0.3",
    "start-server-and-test": "^2.0.1",
    "typescript": "4.9.5"
  }
}
